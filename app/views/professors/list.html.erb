<h1>Professors</h1>

<table class="listing">
	<tr class-"header">
		<th>First Name</th>
		<th>Last Name</th>
		<th>Overall Rating</th>
		<th>Show More</th>
	</tr>

<% @professors.each do |p|%>
	<tr>
		<td><%= p.first_name %></td>
		<td><%= p.last_name %></td>		
		<td><%= p.overall_rating %></td>
		<td><%= link_to("Show", {:action=> 'show', :id =>p.id}, :class => 'action show')%></td>
	</tr>
	<%end%>
</table>